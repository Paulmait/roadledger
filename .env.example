# Supabase Configuration
# Copy this file to .env and fill in your values

# Supabase Project URL (found in Project Settings > API)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Supabase Anonymous Key (found in Project Settings > API)
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# OpenAI API Key (for document extraction - used in Edge Functions only)
# This should NOT be exposed to the client - only used server-side
OPENAI_API_KEY=sk-your-openai-key
